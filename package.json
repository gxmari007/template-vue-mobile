{
  "name": "template-vue-mobile",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode develop",
    "dev:test": "vite --mode test",
    "dev:release": "vite --mode release",
    "dev:production": "vite --mode production",
    "build:develop": "vue-tsc --noEmit && vite build --mode develop",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "build:release": "vue-tsc --noEmit && vite build --mode release",
    "build:production": "vue-tsc --noEmit && vite build --mode production",
    "serve": "vite preview",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "./**/*.{js,ts,tsx,vue}": "eslint"
  },
  "dependencies": {
    "roshan": "^0.0.7",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "^7.0.2",
    "prettier": "^2.3.2",
    "typescript": "^4.3.2",
    "vite": "^2.5.1",
    "vite-plugin-eslint": "^1.3.0",
    "vue-eslint-parser": "^7.10.0",
    "vue-tsc": "^0.2.3"
  }
}
